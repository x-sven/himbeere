      if (%<ERTStopCheck()>) {
        %<PrintERTStopCheckStatus()>
	semGive(startStopSem);
	return(1);
      }
      
      if (semTake(sem,NO_WAIT) != ERROR) {
	logMsg("Rate for SingleRate task too fast.\n",0,0,0,0,0,0);
